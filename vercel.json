{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate.py"
    },
    {
      "src": "/api/smart",
      "dest": "/api/smart_generate.py"
    },
    {
      "src": "/api/tflite",
      "dest": "/api/tflite_generate.py"
    },
    {
      "src": "/api/tflite_test",
      "dest": "/api/tflite_test.py"
    },
    {
      "src": "/api/pytorch",
      "dest": "/api/pytorch_generate.py"
    },
    {
      "src": "/api/model",
      "dest": "/api/model_handler.py"
    },
    {
      "src": "/api/hello",
      "dest": "/api/hello.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "functions": {
    "api/*.py": {
      "maxDuration": 30
    }
  }
}