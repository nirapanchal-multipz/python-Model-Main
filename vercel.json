{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate.py"
    },
    {
      "src": "/api/smart",
      "dest": "/api/smart_generate.py"
    },
    {
      "src": "/api/model",
      "dest": "/api/model_handler.py"
    },
    {
      "src": "/api/hello",
      "dest": "/api/hello.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}